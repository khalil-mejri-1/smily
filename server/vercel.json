{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "./index.js": {
      "includeFiles": [
        "contour.py",
        "fetch.py",
        "requirements.txt",
        "uploads/**", 
        "processed_images/**" 
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}