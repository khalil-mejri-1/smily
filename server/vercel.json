{
  "version": 2,
  "functions": {
    "server/index.js": {
      "runtime": "@vercel/node@3.0.17",
      "includeFiles": "server/contour.py, server/fetch.py, server/requirements.txt, server/uploads/**, server/processed_images/**" 
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ]
}
